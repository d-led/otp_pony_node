ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  otp_pony_node_c_config = debug
endif
ifeq ($(config),release)
  otp_pony_node_c_config = release
endif

PROJECTS := otp_pony_node_c

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

otp_pony_node_c:
ifneq (,$(otp_pony_node_c_config))
	@echo "==== Building otp_pony_node_c ($(otp_pony_node_c_config)) ===="
	@${MAKE} --no-print-directory -C . -f otp_pony_node_c.make config=$(otp_pony_node_c_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f otp_pony_node_c.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   otp_pony_node_c"
	@echo ""
